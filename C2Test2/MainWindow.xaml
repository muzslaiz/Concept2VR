<Window x:Class="C2Test2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C2Test2"
        mc:Ignorable="d"
        Title="Evező" Height="350" Width="525" WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize">
    <Grid Height="320">
        <Button Name="btn_start" Content="Indítás" HorizontalAlignment="Left" Margin="200,44,0,0" VerticalAlignment="Top" Width="125" Height="25" Click="btn_start_Click"/>
        <Button Name="btn_options" Content="Beállítások" HorizontalAlignment="Left" Margin="200,113,0,0" VerticalAlignment="Top" Width="125" Height="25" Click="btn_options_Click"/>
        <Button Name="btn_testconnection" Content="Kapcsolódás teszt" HorizontalAlignment="Left" Margin="200,182,0,0" VerticalAlignment="Top" Width="125" Height="25" Click="btn_testconnection_Click"/>
        <Button Name="btn_exit" Content="Kilépés" HorizontalAlignment="Left" Margin="200,251,0,0" VerticalAlignment="Top" Width="125" Height="25" Click="btn_exit_Click"/>
    </Grid>
</Window>
